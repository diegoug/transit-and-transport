{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Flask: init",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": [
                "db",
                "init"
            ],
            "env": {
                "FLASK_APP": "manage.py",
                "FLASK_DEBUG": "1"
            },
            "jinja": true,
            "justMyCode": false,
            "console": "internalConsole"
        },
        {
            "name": "Flask: migrate",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": [
                "db",
                "migrate"
            ],
            "env": {
                "FLASK_APP": "manage.py",
                "FLASK_DEBUG": "1"
            },
            "jinja": true,
            "justMyCode": false,
            "console": "internalConsole"
        },
        {
            "name": "Flask: upgrade",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": [
                "db",
                "upgrade"
            ],
            "env": {
                "FLASK_APP": "manage.py",
                "FLASK_DEBUG": "1"
            },
            "jinja": true,
            "justMyCode": false,
            "console": "internalConsole"
        },
        {
            "name": "Flask: app",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "run.py",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--host=0.0.0.0"
            ],
            "jinja": true,
            "justMyCode": false,
            "console": "internalConsole"
        }
    ]
}